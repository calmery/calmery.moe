{
  "dependencies": {
    "@sentry/browser": "5.14.2",
    "classnames": "2.2.6",
    "contentful": "7.14.0",
    "parallax-js": "3.1.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-ga": "2.7.0",
    "webp-in-css": "0.3.0"
  },
  "devDependencies": {
    "@svgr/webpack": "5.2.0",
    "@types/autoprefixer": "9.7.1",
    "@types/classnames": "2.2.10",
    "@types/copy-webpack-plugin": "5.0.0",
    "@types/html-webpack-plugin": "3.2.2",
    "@types/jest": "25.1.4",
    "@types/mini-css-extract-plugin": "0.9.1",
    "@types/node": "13.9.1",
    "@types/node-fetch": "2.5.5",
    "@types/optimize-css-assets-webpack-plugin": "5.0.1",
    "@types/react": "16.9.29",
    "@types/react-dom": "16.9.5",
    "@types/terser-webpack-plugin": "2.2.0",
    "@types/webpack-merge": "4.1.5",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "autoprefixer": "9.7.4",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "css-modules-typescript-loader": "4.0.0",
    "es6-promise": "4.2.8",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "file-loader": "5.1.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "license-webpack-plugin": "2.1.4",
    "lint-staged": "10.0.8",
    "mini-css-extract-plugin": "0.9.0",
    "node-fetch": "2.6.0",
    "node-sass": "4.13.1",
    "now": "17.0.4",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "run-script-os": "1.1.1",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "terser-webpack-plugin": "2.3.5",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2"
  },
  "engines": {
    "node": "12.14.1",
    "npm": "6.13.6"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "ignorePatterns": [
      "*.scss.d.ts"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-hooks"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": 0,
      "react/prop-types": 0
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "^~/(.+)": "<rootDir>/src/$1",
      "\\.scss$": "identity-obj-proxy",
      "\\.svg$": "@svgr/webpack"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ]
  },
  "lint-staged": {
    "**/*.(html|json|md|scss|yml)": [
      "npm run lint:static",
      "git add"
    ],
    "**/*.(ts|tsx)": [
      "npm run lint:typescript",
      "git add"
    ]
  },
  "name": "calmery.moe",
  "private": true,
  "scripts": {
    "build": "rimraf build && run-script-os",
    "build:default": "NODE_ENV=production webpack",
    "build:win32": "set NODE_ENV=production webpack",
    "lint": "npm-run-all lint:*",
    "lint:static": "prettier --ignore-path .gitignore --write \"**/*.{html,json,md,scss,yml}\"",
    "lint:typescript": "eslint --ignore-path .gitignore --fix . --ext .ts --ext .tsx",
    "now-build": "npm run build && mv build dist",
    "start": "webpack-dev-server --host 0.0.0.0 --port 3000",
    "test": "jest --coverage"
  },
  "version": "0.1.0"
}
